<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>EditPage</servlet-name>
        <servlet-class>com.deuteriumlabs.dendrite.controller.EditPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditPage</servlet-name>
        <url-pattern>/editPage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SubmitNewPage</servlet-name>
        <servlet-class>com.deuteriumlabs.dendrite.controller.SubmitNewPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitNewPage</servlet-name>
        <url-pattern>/submitNewPage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SubmitNewStory</servlet-name>
        <servlet-class>com.deuteriumlabs.dendrite.controller.SubmitNewStoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitNewStory</servlet-name>
        <url-pattern>/submitNewStory</url-pattern>
    </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
