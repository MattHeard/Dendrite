<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>SubmitRewrite</servlet-name>
        <servlet-class>com.deuteriumlabs.dendrite.controller.SubmitRewriteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitRewrite</servlet-name>
        <url-pattern>/submitRewrite</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SubmitNew</servlet-name>
        <servlet-class>com.deuteriumlabs.dendrite.controller.SubmitNewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitNew</servlet-name>
        <url-pattern>/submitNewStory</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SubmitWrite</servlet-name>
        <servlet-class>com.deuteriumlabs.dendrite.controller.SubmitWriteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitWrite</servlet-name>
        <url-pattern>/submitWrite</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdatePreferences</servlet-name>
        <servlet-class>com.deuteriumlabs.dendrite.controller.UpdatePreferencesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdatePreferences</servlet-name>
        <url-pattern>/updatePreferences</url-pattern>
    </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
